{
  "name": "react-boilerplate",
  "version": "0.0.0",
  "author": "Germ√°n Cutraro",
  "license": "ISC",
  "scripts": {
    "start": "vite",
    "build:development": "vite build --mode development",
    "build:staging": "vite build --mode staging",
    "build:production": "vite build",
    "serve": "vite preview --port 8086",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cypress": "cypress open",
    "lint": "eslint . --fix",
    "jsdoc": "jsdoc -c jsdoc.json",
    "storybook": "start-storybook -c documentation/storybook -p 6006",
    "build:storybook": "build-storybook -c documentation/storybook"
  },
  "dependencies": {
    "connected-react-router": "^6.9.1",
    "history": "4.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.4.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "5.1.2",
    "redux": "^4.1.0",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-viewport": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.8",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "cypress": "^7.2.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jest-dom": "^3.8.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-testing-library": "^4.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "jsdoc-http-plugin": "^0.3.2",
    "lint-staged": "^11.0.0",
    "prettier": "^2.2.1",
    "redux-logger": "^3.0.6",
    "sass": "^1.32.12",
    "serverless-s3-sync": "^1.17.1",
    "storybook": "^6.2.9",
    "vite": "^2.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,spec.js,test.js}": [
      "eslint . --fix"
    ],
    "*.{js,jsx,css,md}": "prettier --write"
  }
}
